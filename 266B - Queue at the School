    #include <iostream>
    using namespace std;
     
    int main() {
    	
    	int n,t,temp;
    	scanf("%d %d",&n,&t);
    	string a;
    	cin>>a;
    	
    	for(int j=0; j<t; j++){
    		for(int i=0; i<a.size()-1; i++){
    			if((a[i]=='B')&&(a[i+1]=='G')){
    				temp=a[i];
    				a[i]=a[i+1];
    				a[i+1]=temp;
    				i++;
    			}
    		}
    	}
    	
    	for(int i=0; i<a.size(); i++)
    		cout<<a[i];
    	cout<<endl;
    	
    	
    	return 0;
    }





