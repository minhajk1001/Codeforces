    #include <iostream>
    #include <cmath>
    using namespace std;
     
    int main() {
    	
    	int n;
    	scanf("%d",&n);
    	
    	if(n==1){
    		cout<<"2"<<endl;
    		return 0;
    	}
    		
    	double x= sqrt(n);
    	int y= sqrt(n);
    	
    	if(fmod(x,y)==0){
    		cout<<y*2<<endl;
    		return 0;
    	}
    	
    	if((fmod(x,y)>0.5))
    		cout<<y*2+2<<endl;
    		
    	else
    		cout<<y*2+1<<endl;
     
    }



