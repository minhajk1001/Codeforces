    #include <iostream>
    #include <algorithm>
    
    using namespace std;
     
    int main() {
    	
    	int n;
    	scanf("%d",&n);
    	
    	string s;
    	cin>>s;
    	
    	char a[n];
    	for(int i=0; i<n; i++)
    		a[i]=s[i];
    		
    	if(n%2!=0){
    		printf("1\n");
    		for(int i=0; i<n; i++)
    			printf("%c",a[i]);
    		printf("\n");
    		return 0;
    	}
    	
    	int cnt1=0,cnt2=0;
    	for(int i=0; i<n; i++){
    		if(a[i]=='1')
    			cnt1++;
    		else
    			cnt2++;
    	}
    	
    	if(cnt1==cnt2){
    		printf("2\n");
    		for(int i=0; i<n-1; i++)
    			printf("%c",a[i]);
    		printf(" %c\n",a[n-1]);
    		return 0;
    	}
    	
    	else{
    		printf("1\n");
    		for(int i=0; i<n; i++)
    			printf("%c",a[i]);
    		printf("\n");
    		return 0;
    		}
    	
    	return 0;
    }




